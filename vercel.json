{
  "version": 2,
  "builds": [
    {
      "src": "vercel_streamlit.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_streamlit.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  },
  "functions": {
    "vercel_streamlit.py": {
      "maxDuration": 60
    }
  }
}